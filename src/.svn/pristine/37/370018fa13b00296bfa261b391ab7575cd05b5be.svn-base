import fetch from 'utils/fetch';


// 保存挂账
export function saveGZData(data) {
  return fetch({
    baseURL: process.env.BASE_API,
    url: '/api/QMYS/SaveGZData',
    method: 'post',
    data
  })
}
// 获取合同所属的挂账信息
export function getGZData(query) {
  return fetch({
    baseURL: process.env.BASE_API,
    url: '/api/QMYS/GetGZData',
    method: 'get',
    params: query
  })
}
